        alert(val);
    }
    //
    function onEven() {
        alert("even");
    }
    function onOdd() {
        alert("odd");
    }
    //1 2 3
    function onTwoOne(val) {
        alert(val);
    }

    function setMoney(val) {
        $("#inputMoney").val(val);
    }

    function sendMoneyClick() {
        $("#inputMoney").val()
    }

</script>
