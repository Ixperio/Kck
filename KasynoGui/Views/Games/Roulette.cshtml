@model KasynoGui.Models.RuletkaViewModel
@{
    ViewBag.Title = "Roulette";
}

<style>
    .header > .options > #agames > .underline {
        width: 100%;
        opacity: 1;
    }
    cont2{
        padding: 0;
        margin: 0;
    }
    main{
        display: grid;
        grid-template-columns: 1fr;
        width: 100%;
        height: 100%;
        justify-items: center;
        align-content: flex-start;
        grid-column-gap: 30px;
    }
    .box {
        background: linear-gradient(#10131A, #0e0e0e);
        border-radius: 15px;
        display: grid;
        grid-template-columns: 1fr;
        padding: 30px;
        padding-top: 10px;
        font-family: 'Pirata One';
        color: #fff;
        width: max-content;
        height: 100%;
        grid-template-rows: min-content 1px min-content 1px;
        grid-row-gap: 10px;
    }
    .box > h4{
        font-size: 2em;
        width: 100%;
        padding: 10px;
        padding-bottom: 0;
        text-align: center;
        height: min-content;
    }
    .vline {
        width: 100%;
        content: '';
        height: 1px;
        background-color: #202020;
    }
    .horizontal{
        display: grid;
        grid-column-gap: 50px;
        width: 100%;
        height: 100%;
    }
    main{
        display: grid;
        grid-template-rows: 1fr 2fr;
        grid-template-columns: 1fr;
        justify-content: center;
        justify-items: center;
        align-content: flex-start;
        align-items: flex-start;
    }
    .data{
        display: grid;
        justify-content: center;
        align-content: flex-start;
    }
    .rouletteboard {
        display: grid;
        grid-template-areas:
            "zero board board board board board board twoone1"
            "zero board board board board board board twoone2"
            "zero board board board board board board twoone3"
            "nothing firsttwelve firsttwelve secondtwelve secondtwelve thirdtwelve thirdtwelve nothing2"
            "nothing oneeighteen even red black odd nineteenthirtysix nothing2";
        width: min-content;
        transform: scale(1.3);
        margin: 40px;
        margin-right: 70px;
        margin-left: 70px;
    }
    .boardobjectyellow {
        background-color: #E7B919;
    }
    .boardobjectgreen {
        background-color: #52AB33;
    }
    .boardobjectred {
        background-color: #D92A2A;
    }
    .boardobjectblack {
        background-color: #000;
    }
    .boardobjectblue {
        background-color: #133373;
    }
    .boardobjectpurple {
        background-color: #702A92;
    }

    .boardobjectnothing{
        background-image: url(@Url.Content("~/Content/Games/Roulette/tlonothing.png"));
        background-repeat: repeat;
        background-size: cover;
        overflow: hidden;
    }

    .dot{
        border-radius: 50%;
        width: 20px;
        height: 20px;
        content: '';
        box-shadow: 0px 0px 2px 2px #fff;
    }

    .boardobject {
        border: 1px solid #fff;
        width: 100%;
        content: '';
        height: 100%;
        box-sizing: border-box;
        color: #fff;
        font-family: 'Black Ops One', serif;
        place-items: center;
        place-content: center;
        font-size: 0.8em;
        display: grid;
        cursor: pointer;
        transition: 0.12s;
    }
    .rouletteboard > .numbers{
        display: grid;
        grid-template-columns: repeat(12, 32px);
        grid-template-rows: repeat(3, 32px);
        box-sizing: border-box;
    }
    .boardobject:hover{
        filter: brightness(1.4);
        transition: 0.2s;
    }
    .moneyplace{
        display: grid;
        grid-template-areas:
            "bt1 bt2 bt3"
            "bt4 bt5 bt6"
            "input input submit"
            "info info .";
        grid-column-gap: 22px;
        grid-row-gap: 22px;
        font-family: 'Black Ops One', serif;
    }
    .fastpickmoney{
        color: #000;
        place-items: center;
        display: grid;
        border-radius: 5px;
        background: linear-gradient(rgba(255,229,0,1),rgba(255,168,0,1));
        outline: none;
        text-align: center;
        width: 100px;
        height: 36px;
        border: none;
    }
    .placebet{
        display: grid;
        grid-template-columns: max-content min-content;
        justify-content: center;
        justify-items: center;
        align-content: flex-start;
        align-items: flex-start;
        grid-column-gap: 40px;
        margin-top: 30px;
    }
    .placebet > .money{
        height: auto;
        display: grid;
        grid-template-columns: 1fr;
        place-items: center;
        padding: 20px;
        grid-row-gap: 5px;
    }
    .myMoney {
        padding: 7px;
        border-radius: 5px;
        background-color: #1D1E1F;
        display: grid;
        place-items: center;
        text-align: center;
        color: #fff;
        font-family: 'Black Ops One', serif;
        grid-template-columns: min-content min-content;
        grid-column-gap: 5px;
        padding-left: 20px;
        padding-right: 20px;
    }
    input[type="number"]::-webkit-inner-spin-button,
    input[type="number"]::-webkit-outer-spin-button {
        -webkit-appearance: none;
        margin: 0;
    }

    input[type="number"] {
        -moz-appearance: textfield; /* Dodaj obsługę dla Firefox */
    }
    .block {
        width: 10vw;
        height: 10vw;
        display: grid;
        font-size: 2em;
        color: #fff;
        font-family: 'Black Ops One';
        text-align: center;
        place-content: center;
        place-items: center;
    }
    .red {
        background-color: #D92A2A;
    }
    .black {
        background-color: #000;
    }
    .green {
        background-color: #52AB33;
    }
    #lastblock{
        transition: linear;
    }
